{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": [
    "cypress"
  ],
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:cypress/recommended"
  ]
}
